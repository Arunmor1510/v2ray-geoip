{
  "input": [
    {
      "type": "ipinfoMMDB",
      "action": "add",
      "args": {
        "uri": "https://github.com/cnartlu/geoip2/releases/latest/download/country.mmdb",
        "wantedList": [
          "europe",
          "North America",
          "hk",
          "sg",
          "tw",
          "jp"
        ]
      }
    },
    {
      "type": "text",
      "action": "add",
      "args": {
        "name": "cn",
        "uri": "https://raw.githubusercontent.com/17mon/china_ip_list/master/china_ip_list.txt",
        "onlyIPType": "ipv4"
      }
    },
    {
      "type": "text",
      "action": "add",
      "args": {
        "name": "cn",
        "uri": "https://raw.githubusercontent.com/gaoyifan/china-operator-ip/ip-lists/china.txt",
        "onlyIPType": "ipv4"
      }
    },
    {
      "type": "text",
      "action": "add",
      "args": {
        "name": "cn",
        "uri": "https://raw.githubusercontent.com/gaoyifan/china-operator-ip/ip-lists/china6.txt",
        "onlyIPType": "ipv6"
      }
    },
    {
      "type": "text",
      "action": "add",
      "args": {
        "name": "cloudflare",
        "uri": "https://www.cloudflare.com/ips-v4"
      }
    },
    {
      "type": "text",
      "action": "add",
      "args": {
        "name": "cloudflare",
        "uri": "https://www.cloudflare.com/ips-v6"
      }
    },
    {
      "type": "text",
      "action": "add",
      "args": {
        "name": "telegram",
        "uri": "https://core.telegram.org/resources/cidr.txt"
      }
    },
    {
      "type": "text",
      "action": "add",
      "args": {
        "name": "anycast",
        "uri": "https://github.com/bgptools/anycast-prefixes/raw/master/anycatch-v4-prefixes.txt"
      }
    },
    {
      "type": "text",
      "action": "add",
      "args": {
        "name": "anycast",
        "uri": "https://github.com/bgptools/anycast-prefixes/raw/master/anycatch-v6-prefixes.txt"
      }
    },
    {
      "type": "private",
      "action": "add"
    }
  ],
  "output": [
    {
      "type": "text",
      "action": "output",
      "args": {
        "outputDir": "./premerge"
      }
    }
  ]
